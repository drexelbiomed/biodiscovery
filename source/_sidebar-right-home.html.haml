.home-panel
  %h3 Upcoming Events
  %ul.unstyled
    - rss_feed.first(8).reverse.first(3).each do |event|
      %li
        %strong= home_event(event, "title")
        %br
        = home_event(event, "speaker")
        %br
        = home_event(event, "time")
  = link_to "Events", "#{http_prefix}events/events_list", class: "more-link"